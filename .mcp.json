{
    "mcpServers": {
        "XcodeMCP": {
            "command": "bunx",
            "args": ["-y", "xcodemcp@latest"],
            "env": {
                "LOG_LEVEL": "INFO",
                "XCODE_MCP_PREFERRED_SCHEME": "swmpc",
                "XCODE_MCP_PREFERRED_XCODEPROJ": "swmpc.xcodeproj"
            }
        },
        "apple-docs": {
            "command": "bunx",
            "args": ["-y", "@kimsungwhee/apple-docs-mcp"]
        }
    }
}
